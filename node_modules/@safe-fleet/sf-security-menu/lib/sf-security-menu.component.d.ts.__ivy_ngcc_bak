import { OnInit } from '@angular/core';
import { Align, Offset } from '@progress/kendo-angular-popup';
import { SfMenuService } from '@safe-fleet/sf-menu';
import { SfSecurityMenuService } from './services/sf-security-menu/sf-security-menu.service';
import { IUnauthorizedPopup } from './interfaces/IMenuConfig';
export declare class SfSecurityMenuComponent implements OnInit {
    private sfSecMenuService;
    private sfMenuService;
    private readonly highlightClass;
    private readonly paddingWidth;
    popupAlign: Align;
    delayTime: number;
    offsetPopup: Offset;
    showOrganizationMsgPopup: boolean;
    organizationItemMenu: HTMLElement;
    unauthorizedPopup: IUnauthorizedPopup;
    constructor(sfSecMenuService: SfSecurityMenuService, sfMenuService: SfMenuService);
    ngOnInit(): void;
    onMenuCollapseOrExpand(): void;
    onResize(): void;
    getOrganizationPosition(): void;
    closeShowOrganizationMsgPopup(): void;
}

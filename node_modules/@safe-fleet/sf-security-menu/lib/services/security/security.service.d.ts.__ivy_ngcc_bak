import { HttpClient } from '@angular/common/http';
import { Observable } from 'rxjs';
import { IApplication } from '../../interfaces/IApplication';
export declare class SecurityService {
    private readonly httpClient;
    private securityEndpoint;
    private settingsEndpoint;
    private securityHeaders;
    private permissions;
    private applicationInfo;
    constructor(httpClient: HttpClient);
    setSecurityInformation(securityEndpoint: string, settingsEndpoint: string, headers: any): void;
    getApplications(): Observable<IApplication[]>;
    getPermissions(): any;
    getInfo(): any;
    getSecurityPermissions(resourceName: string): Observable<any>;
    getApplicationInfo(): Observable<any>;
}

import { OnDestroy, OnInit } from '@angular/core';
import { Align } from '@progress/kendo-angular-popup';
import { MenuConfig, SsoManageAccountConfig } from '../../models/menu-config';
import { MenuItem } from '../../models/menu-item';
import { SfMenuService } from '../../sf-menu.service';
interface Tenant {
    id: string;
    active: boolean;
}
export declare class MenuUserComponent implements OnInit, OnDestroy {
    readonly svc: SfMenuService;
    menuConfig: MenuConfig;
    private configSubs$;
    tenants: Tenant[];
    color: string;
    tenantBg: string;
    collapsed: boolean;
    private collapsedSubs$;
    multipleTenantMenu: boolean;
    anchorAlign: Align;
    popupAlign: Align;
    userActions: any;
    userName: string;
    darkMode: boolean;
    constructor(svc: SfMenuService);
    ngOnInit(): void;
    ngOnDestroy(): void;
    get disableUserNameClick(): boolean;
    changeTenant(tenant: Tenant): void;
    showMultipleTenantMenu(): void;
    hideMultipleTenantMenu(): void;
    userOptionClicked(item: MenuItem): void;
    setDarkMode(): void;
    themeIconClicked(): void;
    onSsoManageAccount(event: MouseEvent, config: SsoManageAccountConfig): void;
    sortMultipleTenant(tenants: string[]): string[];
}
export {};

import { OnInit, OnDestroy } from '@angular/core';
import { MenuConfig } from '../../models/menu-config';
import { SfMenuService } from '../../sf-menu.service';
import { TooltipDirective } from '@progress/kendo-angular-tooltip';
export declare class SfMenuComponent implements OnInit, OnDestroy {
    readonly svc: SfMenuService;
    borderColor: string;
    set config(value: MenuConfig);
    menuConfig: MenuConfig;
    private configSubs$;
    gradient: string;
    color: string;
    collapsed: boolean;
    private collapsedSubs$;
    tooltipDir: TooltipDirective;
    constructor(svc: SfMenuService);
    ngOnInit(): void;
    ngOnDestroy(): void;
    toggleMenu(): void;
}

import { OnDestroy, OnInit } from '@angular/core';
import { MenuConfig } from '../../models/menu-config';
import { MenuItem } from '../../models/menu-item';
import { SfMenuService } from '../../sf-menu.service';
export declare class TopMenuComponent implements OnInit, OnDestroy {
    readonly svc: SfMenuService;
    menuConfig: MenuConfig;
    private configSubs$;
    activeLink: string;
    iconColor: string;
    backgroundColor: string;
    collapsed: boolean;
    private collapsedSubs$;
    get bgColor(): string;
    constructor(svc: SfMenuService);
    ngOnInit(): void;
    ngOnDestroy(): void;
    shortcutClicked(shortcut: MenuItem): void;
    toggleMenu(): void;
}

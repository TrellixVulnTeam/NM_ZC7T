{
  "name": "@progress/kendo-angular-inputs",
  "description": "Inputs Package for Angular",
  "author": "Progress",
  "license": "SEE LICENSE IN LICENSE.md",
  "version": "8.0.9",
  "main": "dist/npm/index.js",
  "module": "dist/fesm5/index.js",
  "es2015_ivy_ngcc": "__ivy_ngcc__/dist/fesm2015/index.js",
  "es2015": "dist/fesm2015/index.js",
  "esm5": "dist/es/index.js",
  "esm2015": "dist/es2015/index.js",
  "jsnext:main": "dist/es/index.js",
  "typings": "dist/es2015/index.d.ts",
  "jsdelivr": "dist/cdn/main.js",
  "unpkg": "dist/cdn/main.js",
  "sideEffects": false,
  "scripts": {
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch --env=jsdom",
    "test:ci": "ci-angular-test",
    "start": "gulp start",
    "e2e:ci": "ci-angular-e2e",
    "e2e_visual:ci": "ci-angular-e2e --visual && ci-commit-snapshots",
    "lint": "gulp lint && gulp lint-docs",
    "api-check": "gulp api-check",
    "build-package": "gulp build-package",
    "semantic-release": "semantic-release pre && semantic-prerelease publish && semantic-release post",
    "ci-semantic-release": "ci-semantic-release",
    "prepublishOnly": "node --eval \"console.error('ERROR: Trying to publish a package that has been compiled by NGCC. This is not allowed.\\nPlease delete and rebuild the package, without compiling with NGCC, before attempting to publish.\\nNote that NGCC may have been run by importing this package into another project that is being built with Ivy enabled.\\n')\" && exit 1"
  },
  "keywords": [
    "Kendo UI",
    "Progress",
    "Angular",
    "Slider",
    "Switch",
    "MaskedTextBox"
  ],
  "peerDependencies": {
    "@angular/animations": "8 - 13",
    "@angular/common": "8 - 13",
    "@angular/core": "8 - 13",
    "@angular/forms": "8 - 13",
    "@progress/kendo-angular-common": "^2.0.0",
    "@progress/kendo-angular-intl": "^3.0.0",
    "@progress/kendo-angular-l10n": "^3.0.0",
    "@progress/kendo-angular-popup": "^4.0.0",
    "@progress/kendo-drawing": "^1.5.12",
    "@progress/kendo-licensing": "^1.0.2",
    "rxjs": "^6.4.0 || ^7.0.0"
  },
  "dependencies": {
    "@progress/kendo-common": "^0.2.1",
    "@progress/kendo-schematics": "^2.0.0",
    "@telerik/kendo-draggable": "^2.0.0",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@angular-devkit/core": "^8.2.0",
    "@angular-devkit/schematics": "^8.2.0",
    "@angular/animations": "^8.2.0",
    "@angular/common": "^8.2.0",
    "@angular/compiler": "^8.2.0",
    "@angular/compiler-cli": "^8.2.0",
    "@angular/core": "^8.2.0",
    "@angular/forms": "^8.2.0",
    "@angular/platform-browser": "^8.2.0",
    "@angular/platform-browser-dynamic": "^8.2.0",
    "@angular/platform-server": "^8.2.0",
    "@angular/router": "^8.2.0",
    "@progress/kendo-angular-buttons": "dev",
    "@progress/kendo-angular-common": "^2.0.0",
    "@progress/kendo-angular-dateinputs": "^5.0.0",
    "@progress/kendo-angular-dropdowns": "^5.0.0",
    "@progress/kendo-angular-e2e": "^3.0.0",
    "@progress/kendo-angular-icons": "^0.4.0",
    "@progress/kendo-angular-intl": "^3.0.0",
    "@progress/kendo-angular-jest-preset": "^2.0.0",
    "@progress/kendo-angular-l10n": "^3.0.0",
    "@progress/kendo-angular-label": "^3.0.0",
    "@progress/kendo-angular-layout": "^6.0.0",
    "@progress/kendo-angular-popup": "^4.0.0",
    "@progress/kendo-angular-progressbar": "^2.0.0",
    "@progress/kendo-angular-tasks": "^19.3.3",
    "@progress/kendo-angular-treeview": "^5.0.0",
    "@progress/kendo-drawing": "^1.5.12",
    "@progress/kendo-licensing": "^1.0.2",
    "@progress/kendo-e2e": "^0.7.0",
    "@progress/kendo-svg-icons": "^0.1.2",

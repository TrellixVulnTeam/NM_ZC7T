import { TabStripTabComponent } from '../models/tabstrip-tab.component';
import { TabStripService } from '../tabstrip.service';
import { LocalizationService } from '@progress/kendo-angular-l10n';
/**
 * @hidden
 */
export declare class TabComponent {
    localization: LocalizationService;
    tabstripService: TabStripService;
    tab: TabStripTabComponent;
    index: number;
    tabStripClosable: boolean;
    tabStripCloseIcon: string;
    hostClasses: boolean;
    readonly activeClass: boolean;
    readonly disabledClass: boolean;
    readonly focusedClass: boolean;
    readonly tabIndex: number;
    constructor(localization: LocalizationService, tabstripService: TabStripService);
    readonly tabClosable: boolean;
    readonly closeButtonClasses: string;
    readonly closeButtonTitle: string;
    closeTab(index: number): void;
}

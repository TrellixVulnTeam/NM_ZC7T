import { ElementRef, Renderer2, NgZone, AfterViewInit, OnDestroy } from '@angular/core';
import { ScrollService } from './tabstrip-scroll.service';
/**
 * @hidden
 */
export declare class TabStripScrollableButtonComponent implements AfterViewInit, OnDestroy {
    host: ElementRef;
    private scrollService;
    private renderer;
    private ngZone;
    btnClasses: boolean;
    readonly prevClass: boolean;
    readonly nextClass: boolean;
    disabled: boolean;
    prev: boolean;
    readonly hostBoundingClientRect: any;
    readonly iconClass: string;
    private _disabled;
    private subs;
    constructor(host: ElementRef, scrollService: ScrollService, renderer: Renderer2, ngZone: NgZone);
    ngAfterViewInit(): void;
    ngOnDestroy(): void;
    toggle(show: boolean): void;
    private clickHandler;
}
